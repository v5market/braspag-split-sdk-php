{
  "name": "v5market/braspag-split-sdk-php",
  "type": "library",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "V5 Market",
      "homepage": "https://github.com/v5market"
    }
  ],
  "keywords": [
    "braspag"
  ],
  "require-dev": {
    "phpunit/phpunit": "^9.3",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "require": {
    "php": "^7.3 | ^8.0",
    "ext-curl": "*",
    "ext-json": "*",
    "monolog/monolog": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "Braspag\\Split\\": "src/",
      "Braspag\\Split\\Test\\": "tests/vendor/"
    }
  },
  "scripts": {
    "phpunit": "vendor/bin/phpunit",
    "phpcs": "vendor/bin/phpcs --colors -s --bootstrap=vendor/autoload.php --standard=PSR1,PSR12 -p --report=full src/"
  }
}
